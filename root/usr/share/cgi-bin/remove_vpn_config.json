#!/bin/sh
# accepted parameters: $file
# include POST/GET READER
. /usr/share/cgi-bin/request
echo "Content-Type: application/json; charset=utf-8"
echo ""
# check file
if [ -z "$file" ];
 then
  echo "{\"status\":\"1\",\"message\":\"No parameters\"}"
  exit 1
fi
# remove
sudo -E /usr/share/cgi-bin/_remove_vpn_config.sh
if [ $? -gt 0 ];
 then
  echo "{\"status\":\"1\",\"message\":\"Error on removing configuration\"}"
 else
  echo "{\"status\":\"0\",\"message\":\"OK\"}"
fi
